db_master_conn_user=${db_master_conn_user:-postgres}
db_master_user="${db_master_conn_user%%@*}"
suffix="${db_master_conn_user##$db_master_user}"

db_conn_user=${db_conn_user:-${db_default_user}${suffix}}
db_user="${db_conn_user%%@*}"
db_database=${db_database:-centerui_production}
db_admin_user=${db_admin_user:-centerui}
db_admin_password=${db_admin_password:-centerui}
db_schema=${db_schema:-centerui_production}

declare -A -x XRD_CS_DB
XRD_CS_DB[DATABASE_NAME]=${db_database}
XRD_CS_DB[MASTER_USER]=${db_master_user}
XRD_CS_DB[ADMIN_USER]=${db_admin_user}
XRD_CS_DB[ADMIN_PASSWORD]=${db_admin_password}
XRD_CS_DB[SCHEMA_NAME]=${db_schema}
